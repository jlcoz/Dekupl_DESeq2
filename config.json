{
  "tmp_dir": "/tmp/",
  "fastq_dir": "data",
  "nb_threads": 10,
  "max_memory": 8000,
  "max_disk": 10000,
  "kmer_length": 31,
  "lib_type": "rf",
  "r1_suffix": "_R1_fastq.gz", 
  "r2_suffix": "_R2_fastq.gz",
   "data_compute_norm_factors": "raw-counts",
  "diff_method": "Ttest_diff_method.R",
  "chunk_size":500,

  "dekupl_counter": {
    "min_recurrence": 6,
    "min_recurrence_abundance": 5
  },

  "Ttest": {
